    "illuminate/config": ">=8.83.23"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.5.24",
    "mockery/mockery": ">=1.5.0",
    "symfony/var-dumper": ">=5.4.11"
  },
  "autoload": {
